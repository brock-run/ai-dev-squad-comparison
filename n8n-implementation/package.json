{
  "name": "n8n-ai-dev-squad",
  "version": "1.0.0",
  "description": "AI Development Squad implementation using n8n",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js"
  },
  "dependencies": {
    "n8n": "^1.106.3",
    "n8n-nodes-base": "^1.104.3",
    "dotenv": "^16.0.0",
    "axios": "^1.6.0",
    "octokit": "^3.1.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)",
      "**/tests/**/*test_*.js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "agents/**/*.{js,ts}",
      "workflows/**/*.{js,ts}"
    ],
    "coverageDirectory": "coverage"
  }
}