{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Ollama": {
    "BaseUrl": "http://localhost:11434",
    "Models": {
      "Architect": "llama3.1:8b",
      "Developer": "codellama:13b",
      "Tester": "llama3.1:8b"
    }
  },
  "GitHub": {
    "ApiToken": "your_github_token_here",
    "Username": "your_github_username",
    "Repository": "your_repository_name"
  },
  "SemanticKernel": {
    "PluginsDirectory": "./Plugins",
    "EnableHumanFeedback": true
  },
  "ModelConfiguration": {
    "MaxTokens": 4096,
    "Temperature": 0.7
  }
}